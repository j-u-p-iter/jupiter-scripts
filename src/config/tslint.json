{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules":{
    "prettier": true,
    "no-console": false,
    "object-literal-sort-keys": "false",
    "no-empty": false
  },
  "jsRules": {
    "prettier": true,
    "no-console": false,
    "object-literal-sort-keys": false,
    "no-empty": false
  },
  "linterOptions": {
    "exclude": [
      "*.json",
      "**/*.json"
    ]
  }
}
