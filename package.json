{
  "name": "@j.u.p.iter/jupiter-scripts",
  "version": "0.0.0-semantically-released",
  "description": "Handy-dandy scripts ;)",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "bin": {
    "jupiter-scripts": "dist/lib/index.js"
  },
  "scripts": {
    "tsnode": "ts-node -P ./src/config/tsconfig.json",
    "build": "node src build --cli --allowJS",
    "build:watch": "watch 'yarn build && yarn changePermission' ./src",
    "changePermission": "chmod +x dist/lib/index.js",
    "test": "node src test --allowJs",
    "lint": "node src lint --format codeFrame --project tsconfig.json",
    "lint:fix": "yarn run lint --fix --allowJs",
    "commit": "node src precommit --allowJs"
  },
  "repository": "git@github.com:j-u-p-iter/jupiter-scripts.git",
  "files": [
    "dist"
  ],
  "author": "Leontyev Kirill <pn79122821224@gmail.com> (j.u.p.iter)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/j-u-p-iter/jupiter-scripts/issues"
  },
  "homepage": "https://github.com/j-u-p-iter/jupiter-scripts#readme",
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@hapi/hapi": "^19.1.1",
    "@octokit/auth-basic": "^1.4.2",
    "@octokit/rest": "^17.2.0",
    "@types/jest": "^23.3.10",
    "arrify": "^1.0.1",
    "await-to-js": "^2.1.1",
    "axios": "^0.19.2",
    "browserslist": "^4.1.1",
    "chalk": "^4.0.0",
    "clear": "^0.1.0",
    "clui": "^0.3.6",
    "commitizen": "^3.0.5",
    "concurrently": "^4.0.1",
    "conf": "^6.2.3",
    "cross-spawn": "^6.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "dotenv": "^8.2.0",
